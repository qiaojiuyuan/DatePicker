<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <attr name="datePickerType" format="string"/>
    <declare-styleable name="DatePickerView">
        <attr name="datePickerType"/>
    </declare-styleable>
    <style name="time_dialog" parent="android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowBackground">@color/ahlib_color_a1_10</item>
    </style>

    <style name="mypopwindow_anim_style">
        <item name="android:windowEnterAnimation">@anim/pop_in_animation</item>
        <!-- 指定显示的动画xml -->

        <item name="android:windowExitAnimation">@anim/pop_out_animation</item>
        <!-- 指定消失的动画xml -->
    </style>
</resources>
